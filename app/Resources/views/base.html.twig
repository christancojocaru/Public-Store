<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        {% block title %}
            <title>Cristian Store</title>
        {% endblock %}
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta content="width=device-width, initial-scale=1" name="viewport" />
            <meta content="Cristian Store" name="description" />
            <meta content="Cristian Cojocaru" name="author" />
        {% block stylesheets %}
            <link href="{{ asset('css/font-awesome.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/simple-line-icons.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/bootstrap-switch.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/components.min.css') }}" rel="stylesheet" id="style_components" type="text/css" />
            <link href="{{ asset('css/plugins.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/layout.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/bootstrap-fileinput.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('css/style.css') }}" rel="stylesheet" type="text/css" />
        {% endblock %}
    </head>
    {% block container %}
        <body>
            <div class="wrapper">
                {% block header %}
                    {% include 'header.html.twig' %}
                {% endblock %}
                {% block body %}{% endblock %}
                {% block footer %}
                    <div>
                        <p class="copyright"> 2019 &copy; Metronic Theme By
                            <a target="_blank" href="http://keenthemes.com">Keenthemes</a> &nbsp;|&nbsp;
                            <a target="_blank" href="#">Cristian Cojocaru</a>
                        </p>
                        <a href="#index" class="go2top">
                            <i class="icon-arrow-up"></i>
                        </a>
                    </div>
                {% endblock %}
            </div>
            {% block javascripts %}
                <script src="{{ asset('js/jquery.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/bootstrap.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/js.cookie.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/jquery.slimscroll.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/jquery.blockui.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/bootstrap-switch.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/app.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/layout.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset('js/bootstrap-fileinput.js') }}" type="text/javascript"></script>

                <!-- Reactjs -->
                <script src="https://unpkg.com/react@16/umd/react.development.js" crossorigin></script>
                <script src="https://unpkg.com/react-dom@16/umd/react-dom.development.js" crossorigin></script>
                <script src="{{ asset('js/reactjs/myreact.js') }}"></script>

                <!-- My js Functions -->
                <script src="{{ asset('js/functions.js') }}" type="text/javascript"></script>


                {% if is_granted("ROLE_ADMIN") %}
                    <script src="{{ asset('js/jquery-ui.min.js') }}" type="text/javascript"></script>

                {#                    <script src="{{ asset('js/ui-modals.min.js') }}" type="text/javascript"></script>#}
                {% endif %}
            {% endblock %}
        </body>
    {% endblock %}
</html>